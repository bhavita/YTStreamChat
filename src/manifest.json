{
	"name": "YTStreamChat",
	"version": "1.0.0",
	"description": "Enchance youtube stream experience by using emotes",
	"manifest_version": 2,
	"content_scripts": [
		{
			"js": [
				"init.js"
			],
			"matches": [
				"https://www.youtube.com/*"
			]
		}
	],
	"options_ui": {
		"page": "options.html"
	},
	"permissions": [
		"storage",
		"https://api.twitch.tv/*",
		"https://api.betterttv.net/*",
		"https://api.twitchemotes.com/*",
		"https://https://cdn.jsdelivr.net/*"
	]
}